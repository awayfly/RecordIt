<view class='containerC'>
  <view class='enterList'>

    <view class=' imageList '>
      <view class='imageData'>
        <image src=' ../../image/bar-chart.svg ' class='imageSize' bindtap='charts'></image>
      </view>
      <view class='center-line'>
        <text class='content-text'>曲线</text>
      </view>
    </view>
    <view class=' imageList '>
      <view class='imageData'>
        <image src=' ../../image/edit.svg ' class='imageSize' bindtap='record'></image>
      </view>
      <view class='center-line'>
        <text class='content-text'>记录</text>
      </view>
    </view>
    <view class=' imageList '>
      <view class='imageData'>
        <image src=' ../../image/nearby.svg ' class='imageSize'></image>
      </view>
      <view class='center-line'>
        <text class='content-text'>附近</text>
      </view>
    </view>
  </view>

  <view class="section">
    <view class="section__title">时间选择器</view>
    <picker mode="time" value="{{time}}" start="00:00" end="23:59" bindchange="bindTimeChange">
      <view class="picker">
        当前选择: {{time}}
      </view>
    </picker>
  </view>

  <form bindsubmit='bindsubmitTap'>
    <button style="margin:30rpx;" bindtap="chooseimage">获取图片</button>
    <!-- <camera device-position='back' flash='off' binderror='error' style='width:100%;height:300px;'></camera> -->
    <image src='{{tempFilePaths}}' style="width:100%;height:300px;"></image>
    <button style="margin:30rpx;" type='primary' bindtap="takePhoto">拍照</button>
    <!-- <image src="{{tempFilePaths }}" mode="aspecFill" style="width: 100%; height: 450rpx"/>  -->
    <image mode='widthFix' src='{{src}}'></image>
    <button form-type='submit'>提交</button>
  </form>
</view>